<div id="post">
  <h1><%=page.title%></h1>
  <p><%=page.id%></p>
  <p>
    <% if (page.categories && page.categories.length){ %>
      <%- list_categories(page.categories, {
        show_count: false,
        style:''
      }) %>
    <% } %>
  </p>
  <hr />
  <%-page.content%>
</div>
<%-partial('partials/paginator')%>